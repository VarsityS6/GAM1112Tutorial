// room script file

function oNotMug_Look(Object *theObject, CursorMode mode)
{
Display("That looks like my cup, but not?");
cEgo.Say("Woah?");
}

function oNotMug_Interact()
{
  cEgo.Walk(61, 145, eBlock, eWalkableAreas);
  cEgo.FaceDirection(eDirectionUp);
  cEgo.LockView(2);
  cEgo.Animate(0, 5, eOnce, eBlock, eForwards);
  cEgo.UnlockView();
  oNotMug.Visible=false;
  apickup.Play();
  Display("The not mug was picked up?");
  cEgo.AddInventory(iNotMug);
  GiveScore(5);
}